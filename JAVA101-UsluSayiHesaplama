import java.util.*;

public class Main {
    public static void main(String[] args) {
        int n;
        int k;
        int i = 1;
        int sum = 1;

        Scanner input = new Scanner(System.in);

        System.out.print("Üssü alınacak sayı: ");
        n = input.nextInt();

        System.out.print("Üs olacak sayı: ");
        k = input.nextInt();

        for (i = 1; i <= k; i++) {
            sum *= n;
        }
        System.out.println(n + " üzeri " + k + " = " + sum);
    }
}
